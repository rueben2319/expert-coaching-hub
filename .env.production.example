# Production Environment Variables
# Copy this to .env.production and update with your values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# Withdrawal System Configuration
VITE_MAX_WITHDRAWAL=10000
VITE_MIN_WITHDRAWAL=10
VITE_DAILY_WITHDRAWAL_LIMIT=50000
VITE_CREDIT_AGING_DAYS=3
VITE_RATE_LIMIT_PER_HOUR=5
VITE_CONVERSION_RATE=100

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# PayChangu Payment System
VITE_PAYCHANGU_API_URL=https://api.paychangu.com
# Note: PayChangu secret should be in Supabase secrets, not here

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Application Settings
VITE_APP_NAME=Expert Coaching Hub
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Monitoring (Optional)
VITE_SENTRY_DSN=your-sentry-dsn-if-using-sentry
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
